<h2>All Jobs</h2>
<table>
  <thead>
    <tr>
      <td>ID</td>
      <td>Name</td>
      <td>Type</td>
      <td>Datacenters</td>
      <td>Status</td>
      <td>Periodic</td>
      <td>Parameterized</td>
    </tr>
  </thead>
  <tbody>
    {{#each model as |job|}}
      <tr>
        <td><a href="{{href-to "jobs.job" job.id}}">{{job.id}}</a></td>
        <td>{{job.name}}</td>
        <td>{{job.type}}</td>
        <td>{{job.datacenters}}</td>
        <td>{{job.status}}</td>
        <td>{{job.periodic}}</td>
        <td>{{job.parameterized}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
